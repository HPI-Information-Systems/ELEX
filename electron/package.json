{
  "name": "elex",
  "version": "1.0.0",
  "private": true,
  "main": "main.js",
  "repository": "https://github.com/HPI-Information-Systems/ELEX",
  "author": "Benjamin Feldmann <benjamin-feldmann@web.de>",
